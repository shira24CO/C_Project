#define _CRT_SECURE_NO_WARNINGS 
#include <stdio.h>

int main()
{
	char tav;
	printf("Enter a Char: \n");
	scanf_s("%c", &tav);
	if (tav >= 'a' && tav <= 'z')
		printf("%c", tav - 'a' + 'A');
	else if (tav >= 'A' && tav <= 'Z')
		printf("%c", tav - 'A' + 'a');
	else printf("%c", tav);
	return 0;
}
